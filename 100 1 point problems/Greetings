using System;
using System.Collections;
using System.Diagnostics;

namespace KattisCsharp
{
    class Program
    {
        static void Main(string[] args)
        {

            string eee = "";
            string input = Console.ReadLine();
            int numOfE = input.Length - 2;
            Console.WriteLine("h" + eee.Substring(0, numOfE * 2) + "y");

        }

        
    }
}