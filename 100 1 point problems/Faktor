using System;
using System.Collections;
using System.Diagnostics;

namespace KattisCsharp
{
    class Program
    {
        static void Main(string[] args)
        {
            

            double[] input = Array.ConvertAll(Console.ReadLine().Split(' '), x => Convert.ToDouble(x));
            input[1] = input[1] - 1 + 0.01;
            Console.WriteLine(Math.Ceiling(input[0] * input[1]));

        }

        
    }
}