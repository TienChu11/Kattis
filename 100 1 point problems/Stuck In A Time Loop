import java.util.*;


import java.io.File;
import java.lang.*;

public class Main {
    public static
    void main(String[] args){

        Scanner sc = null;

        // real
        sc = new Scanner(System.in);

        int n = sc.nextInt();
        for(int i = 1; i <= n; i++){
            System.out.println(i + " Abracadabra");

        }
    }
}