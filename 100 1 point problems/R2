using System;

namespace KattisCsharp
{
    class Program
    {
        static void Main(string[] args)
        {
            string input = Console.ReadLine();
            int[] values = Array.ConvertAll(input.Split(' '), x => int.Parse(x));
            int result = 2 * values[1] - values[0];
            Console.Write(result);
        }
    }
}